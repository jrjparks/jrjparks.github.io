{
  "extends": ["tslint:recommended"],
  "rules": {
    "align": [false],
    "export-name": false,
    "indent": [true, "spaces", 2],
    "missing-jsdoc": false,
    "newline-before-return": false,
    "no-any": false,
    "no-console": false,
    "no-default-export": false,
    "no-import-side-effect": false,
    "no-relative-imports": false,
    "no-reference": false,
    "ordered-imports": [false],
    "prefer-type-cast": false,
    "semicolon": [false],
    "typedef": [false]
  }
}
